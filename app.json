{
  "expo": {
    "scheme": "kazibufasapp",
    "name": "KazibufastApp",
    "slug": "subscribers-application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logokazbu.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/kazi.png",
      "resizeMode": "contain",
      "backgroundColor": "#21C7B9"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "INTERNET",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logokazbu.png",
        "backgroundColor": "#21C7B9"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.joshuasalabe.kazibufastclientside"
    },
    "web": {
      "favicon": "./assets/images/logokazbu.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "627c5303-73a7-4717-8dd1-fcf2cb3aadf1"
      }
    },
    "owner": "joshuasalabe"
  }
}
